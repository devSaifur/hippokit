    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "14.2.3",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
