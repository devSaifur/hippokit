    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
